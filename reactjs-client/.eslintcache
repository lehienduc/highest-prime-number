;\n    } else {\n      setValidated(false);\n    }\n    setNumber(e.target.value);\n  }\n\n  return (\n    <div className=\"App\">\n      <header className=\"App-header\">\n        <h1 className=\"mb-4\">Find highest prime number</h1>\n        <Form noValidate validated={validated} onSubmit={handleSubmit}>\n          <Form.Row>\n            <Form.Group as={Col} controlId=\"formGridNumber\">\n              <Form.Label>Number</Form.Label>\n              <Form.Control type=\"number\" inputMode=\"numeric\" placeholder=\"Enter number\" value={number}\n              onChange={handleNumberChange} required/>\n              <Form.Control.Feedback type=\"invalid\">\n              Please enter a number.\n            </Form.Control.Feedback>\n            </Form.Group>\n\n            <Form.Group as={Col} controlId=\"formGridResult\">\n              <Form.Label>Highest Prime</Form.Label>\n              <Form.Control type=\"text\" readOnly value={result}/>\n            </Form.Group>\n          </Form.Row>\n          <Button size=\"lg\" variant=\"primary\" type=\"submit\" block disabled={!validated}>\n            Submit\n          </Button>\n        </Form>\n        <img src={logo} className=\"App-logo\" alt=\"logo\" />\n      </header>\n    </div>\n  );\n}\n\nexport default App;\n","/Users/duc/Working/Stably/Go/highest-prime-number/reactjs-client/src/index.js",[],{"ruleId":"16","severity":1,"message":"17","line":4,"column":17,"nodeType":"18","messageId":"19","endLine":4,"endColumn":26},"no-unused-vars","'useEffect' is defined but never used.","Identifier","unusedVar"]